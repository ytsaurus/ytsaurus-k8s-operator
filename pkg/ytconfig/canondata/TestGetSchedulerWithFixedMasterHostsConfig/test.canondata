{
    "address_resolver"={
        "enable_ipv4"=%false;
        "enable_ipv6"=%true;
        retries=1000;
    };
    "solomon_exporter"={
        host="{POD_SHORT_HOSTNAME}";
        "instance_tags"={
            pod="{K8S_POD_NAME}";
        };
    };
    logging={
        writers={
            info={
                type=file;
                "file_name"="/var/log/scheduler.info.log";
                format="plain_text";
                "enable_system_messages"=%true;
            };
            stderr={
                type=stderr;
                format="plain_text";
                "enable_system_messages"=%true;
            };
        };
        rules=[
            {
                "min_level"=info;
                writers=[
                    info;
                ];
                family="plain_text";
            };
            {
                "min_level"=error;
                writers=[
                    stderr;
                ];
                family="plain_text";
            };
        ];
        "flush_period"=3000;
    };
    "monitoring_port"=10011;
    "rpc_port"=9011;
    "timestamp_provider"={
        addresses=[
            "host1.external.address:9010";
            "host2.external.address:9010";
            "host3.external.address:9010";
        ];
    };
    "cluster_connection"={
        "cluster_name"=test;
        "primary_master"={
            addresses=[
                "host1.external.address:9010";
                "host2.external.address:9010";
                "host3.external.address:9010";
            ];
            peers=[
                {
                    address="host1.external.address:9010";
                    voting=%true;
                };
                {
                    address="host2.external.address:9010";
                    voting=%true;
                };
                {
                    address="host3.external.address:9010";
                    voting=%true;
                };
            ];
            "cell_id"="65726e65-ad6b7562-259-79747361";
        };
        "discovery_connection"={
            addresses=[
            ];
        };
        "master_cache"={
            addresses=[
                "host1.external.address:9010";
                "host2.external.address:9010";
                "host3.external.address:9010";
            ];
            "cell_id"="65726e65-ad6b7562-259-79747361";
            "enable_master_cache_discovery"=%false;
        };
    };
    "cypress_annotations"={
        "k8s_node_name"="{K8S_NODE_NAME}";
        "k8s_pod_name"="{K8S_POD_NAME}";
        "k8s_pod_namespace"="{K8S_POD_NAMESPACE}";
        "physical_host"="{K8S_NODE_NAME}";
    };
    scheduler={
        "operations_cleaner"={
            "enable_operation_archivation"=%false;
        };
    };
}